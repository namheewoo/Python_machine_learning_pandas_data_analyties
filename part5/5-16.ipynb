   "source": [
    "import pandas as pd\n",
    "\n",
    "dates = ['2019-01-01', '2020-03-01', '2021-06-01']\n",
    "ts_dates = pd.to_datetime(dates)\n",
    "print(ts_dates)\n",
    "\n",
    "pr_day = ts_dates.to_period(freq=\"D\")\n",
    "print(pr_day)\n",
    "pr_month = ts_dates.to_period(freq='M')\n",
    "print(pr_month)\n",
    "pr_year = ts_dates.to_period(freq='Y')\n",
    "print(pr_year)"
   ]
  }
